{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/extensions":["error","ignorePackages"],
        "no-console": 0,
        "no-restricted-syntax": 0,
        "class-methods-use-this": 0
    }
}
